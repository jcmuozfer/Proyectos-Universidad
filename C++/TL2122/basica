#include <iostream>
using namespace std;
int main(){

	// Inicio del bloque de código
	cout << "Esto es una cadena constante que se  tal cual.";
	cout << 9;
	cout << falso
;
	cout << endl;
	cout << cierto
;
	cout << endl;
	cout << endl;
	cout << "lunes";
	cout << " ";
	cout << 2;
	cout << " ";
	cout << 4;
	cout << endl;
	cout << "domingo";
	cout << " ";
	cout << 13;
	cout << " ";
	cout << 21.07;
	cout << endl;
	cout << endl;
	cout << "lunes";
	cout << endl;
	cout << "martes";
	cout << endl;
	cout << "miercoles";
	cout << endl;
	cout << "jueves";
	cout << endl;
	cout << "viernes";
	cout << endl;
	cout << "Dias laborables";
	cout << endl;
	cout << "Adios";
	// Final del bloque de código

	return 0;
}
